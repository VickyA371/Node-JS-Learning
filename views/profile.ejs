<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Profile Page</title>
</head>
<body>
    <h1>Welcome, <%= user.name %></h1>
    <h3>Email <%= user.email %></h3>
    <h3>Phone <%= user.mobile %></h3>
    <ul>
        <% user.skills.forEach((skill) => { %>
            <li>
                <%= skill %>
            </li>
        <% }) %>
    </ul>
</body>
</html>